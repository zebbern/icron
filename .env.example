# Required for Together
TOGETHER_API_KEY=
# Alternate key name supported by Together
TOGETHERAI_API_KEY=
TOGETHER_API_BASE=

# Optional if using OpenRouter fallback
OPENROUTER_API_KEY=

# Model selection
MODEL=moonshotai/Kimi-K2.5

# Web search
WEBSEARCH_API_KEY=

# Telegram
TELEGRAM_TOKEN=
TELEGRAM_ALLOW_FROM=
TELEGRAM_ENABLED=true

# WhatsApp
WHATSAPP_ENABLED=false

# Nanobot paths (Railway volume)
NANOBOT_DATA_DIR=/Nano/.nanobot
NANOBOT_WORKSPACE=/Nano/workspace
NANOBOT_SHELL_CWD=/

# Behavior
NANOBOT_WRITE_CONFIG=auto
NANOBOT_RESTART_ON_SAVE=1
NANOBOT_HTTP_ENABLED=1
NANOBOT_VERBOSE=0